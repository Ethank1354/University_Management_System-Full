<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="engg1420_project.universitymanagementsystem.ViewEventsController">
    <ComboBox fx:id="eventComboBox" layoutX="100.0" layoutY="20.0" promptText="Select Event to View"/>
    <Button text="View Event" layoutX="100.0" layoutY="60.0" onAction="#viewEvent"/>
    <Label fx:id="eventDetailsLabel" layoutX="100.0" layoutY="100.0" />
</AnchorPane>
